{
  "manifestVersion": "2.2",
  "contracts": {
    "ChaiNucleus": "ChaiNucleus",
    "ChargedParticles": "ChargedParticles",
    "ChargedParticlesEscrow": "ChargedParticlesEscrow",
    "ChargedParticlesERC1155": "ChargedParticlesERC1155"
  },
  "dependencies": {
    "@openzeppelin/contracts-ethereum-package": "^2.4.0"
  },
  "name": "charged-particles",
  "version": "0.1.1",
  "telemetryOptIn": true,
  "compiler": {
    "manager": "openzeppelin",
    "solcVersion": "0.5.16",
    "compilerSettings": {
      "optimizer": {
        "enabled": true,
        "runs": "200"
      }
    },
    "artifactsDir": "build/contracts",
    "contractsDir": "contracts",
    "typechain": {
      "enabled": false
    }
  }
}
